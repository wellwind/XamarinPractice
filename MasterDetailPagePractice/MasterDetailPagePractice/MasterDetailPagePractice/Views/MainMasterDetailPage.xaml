<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="MasterDetailPagePractice.Views.MainMasterDetailPage">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu" BackgroundColor="Gray">
            <StackLayout
                VerticalOptions="Center"
                HorizontalOptions="Center">
                <Button Text="Button 1" Command="{Binding GoPage1Command}"/>
                <Button Text="Button 2" Command="{Binding GoPage2Command}"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>